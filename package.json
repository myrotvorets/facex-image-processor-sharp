    "c8": "^10.1.3",
    "eslint-formatter-gha": "^1.6.0",
    "node-reporter-gha": "^2.0.5",
    "node-reporter-sonarqube": "^1.0.3",
    "sharp": "^0.34.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "@myrotvorets/facex-base": "^1.0.1 || ^2.0.0",
